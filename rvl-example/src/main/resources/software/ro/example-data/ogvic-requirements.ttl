 ##################### ##################### ##################### ##################### ##################### #####################

@prefix asserted_rest:  <http://purl.org/ro/asserted_rest/> .
@prefix cs:      <http://purl.org/ro/cs#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix html:    <http://uispin.org/html#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ro:      <http://purl.org/ro/ont#> .
@prefix ro-ex:   <http://purl.org/ro/semvis-example/> .
@prefix ro-ui:   <http://purl.org/ro/ui#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix sp:      <http://spinrdf.org/sp#> .
@prefix spin:    <http://spinrdf.org/spin#> .
@prefix spl:     <http://spinrdf.org/spl#> .
@prefix tui:     <http://uispin.org/tui#> .
@prefix ui:      <http://uispin.org/ui#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ro:Challenge
      rdfs:label "Problems / Challenges"^^xsd:string ;
      ro-ex:abbreviationInitial
              "P"^^xsd:string ;
      ui:instanceView
              [ rdf:type html:Div ;
                html:class "explained_item"^^xsd:string ;
                ui:child
                        [ rdf:type html:Div ;
                          html:class "label"^^xsd:string ;
                          ui:child
                                  [ rdf:type ui:TextNode ;
                                    ui:childIndex 0 ;
                                    ui:text [ rdf:type ui:label ;
                                              sp:arg1 spin:_this
                                            ]
                                  ] ;
                          ui:childIndex 0
                        ] ;
                ui:child
                        [ rdf:type html:Div ;
                          html:class "description"^^xsd:string ;
                          ui:child
                                  [ rdf:type ui:TextNode ;
                                    ui:childIndex 0 ;
                                    ui:text [ rdf:type spl:object ;
                                              sp:arg1 spin:_this ;
                                              sp:arg2 dcterms:description
                                            ]
                                  ] ;
                          ui:childIndex 1
                        ] ;
                ui:child
                        [ rdf:type tui:CommentBox ;
                          tui:subject spin:_this ;
                          ui:childIndex 2
                        ] ;
                ui:child
                        [ rdf:type html:Div ;
                          ui:child
                                  [ rdf:type html:Span ;
                                    html:class "skosEditorialNote"^^xsd:string ;
                                    ui:child
                                            [ rdf:type ui:TextNode ;
                                              ui:childIndex 0 ;
                                              ui:text [ rdf:type spl:object ;
                                                        sp:arg1 spin:_this ;
                                                        sp:arg2 skos:editorialNote
                                                      ]
                                            ] ;
                                    ui:childIndex 0
                                  ] ;
                          ui:childIndex 3
                        ] ;
                ui:child
                        [ rdf:type html:Span ;
                          html:class "explanation"^^xsd:string ;
                          ui:child
                                  [ rdf:type ui:TextNode ;
                                    ui:childIndex 0 ;
                                    ui:text "fulfilled (partly) by the following contributions:"^^xsd:string
                                  ] ;
                          ui:childIndex 4
                        ] ;
                ui:child
                        [ rdf:type ro-ui:Unordered_Object_List ;
                          ro-ui:objectType ro-ex:ThesisContribution ;
                          ro-ui:relation ro-ex:solvedBy ;
                          ro-ui:subjectUI spin:_this ;
                          ui:childIndex 5
                        ] ;
                ui:priority 2000
              ] .

ro:isRefinementOf
      owl:inverseOf ro:refinesTo .

ro:refinesTo
      owl:inverseOf ro:isRefinementOf .

ro-ex:
      rdf:type owl:Ontology ;
      owl:imports dcterms: , asserted_rest: , <http://purl.org/ro/cs> , <http://purl.org/ro/ont> , <http://purl.org/ro/ui> , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

ro-ex:AVM
      rdf:type ro-ex:ThesisContribution ;
      rdfs:label "AVM"^^xsd:string ;
      ro-ex:fulfills ro-ex:IntrospectCreatedVisualisation ;
      ro-ex:printOrder "3"^^xsd:float ;
      ro-ex:solvesProblem ro-ex:Composition_of_Mappings_insufficient , ro-ex:No_Sharing_and_Reuse_of_Visual_Encodings , ro-ex:PlatformVariability , ro-ex:Single_Visual_Paradigm ;
      skos:editorialNote "Alternatives to AVM as a solution?"^^xsd:string .

ro-ex:AnalysisStateOfTheArtApproachesAndLanguages
      rdf:type ro-ex:ThesisContribution ;
      rdfs:label "Analysis of the State of the Art"^^xsd:string ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:Benefits_of_interaction_for_information_visualisation_not_exploited
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:label "Benefits of interaction for information visualisation not exploited"^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:ConfigurationResultsInstantlyShown , ro-ex:DataFiltering , ro-ex:Guidance4ViewTransformations , ro-ex:Interaction ;
      ro-ex:printOrder "20"^^xsd:float .

ro-ex:BothTBoxAndABoxVisualisationsAreRequired
      rdf:type ro-ex:CaseStudyOntologyAnalysisObservation ;
      rdfs:label "Both TBox and ABox visualisations are required"^^xsd:string .

ro-ex:Can_visualisation_benefit_from_the_additional_complexity_of_ontological_data
      rdf:type ro-ex:ResearchQuestion ;
      rdfs:label "Can visualisation benefit from the additional complexity of ontological data?"^^xsd:string ;
      ro:isRefinementOf ro-ex:How_can_we_support_the_creation_of_tailor-made_efficient_and_reusable_visualisations_for_ontological_data ;
      ro-ex:justifiedBy ro-ex:Ontology_Specifics_Not_Exploited , ro-ex:Single_Visual_Paradigm ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:CaseStudyObservation
      rdf:type owl:Class ;
      rdfs:subClassOf ro:Influencer .

ro-ex:CaseStudyOntologyAnalysisObservation
      rdf:type owl:Class ;
      rdfs:label "Influencer 1"^^xsd:string ;
      rdfs:subClassOf ro-ex:CaseStudyObservation .

ro-ex:ComposabilityOfMappings
      rdf:type ro-ex:VariabilityShareabilityAndComposabilityLanguageRequirement ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:ReusabilityOfMappings ;
      ro-ex:printOrder "9"^^xsd:float .

ro-ex:ComposabilityOfTheDefinedMapping
      rdf:type ro:FunctionalRequirement , ro-ex:VariabilityReuseAndComposabilityRequirement ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Composition_of_Mappings_insufficient ;
      ro-ex:justifies ro-ex:IntrospectCreatedVisualisation , ro-ex:ShareabilitySupportedByTheLanguage ;
      ro-ex:printOrder "6"^^xsd:float .

ro-ex:Composition_of_Mappings_insufficient
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      dcterms:description "Visualisation authors cannot compose their own mappings with existing ones."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:ComposabilityOfTheDefinedMapping , ro-ex:VisualStructureVariability ;
      ro-ex:printOrder "4"^^xsd:float ;
      ro-ex:solvedBy ro-ex:AVM , ro-ex:RVL , ro-ex:SemVisApproach .

ro-ex:ConfigurationResultsInstantlyShown
      rdf:type ro:FunctionalRequirement , ro-ex:OptionalFeature ;
      ro-ex:justifiedBy ro-ex:Benefits_of_interaction_for_information_visualisation_not_exploited , ro-ex:Interaction ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:ConsiderComplexSemanticsOfOntologyForVisualMapping
      rdf:type ro:FunctionalRequirement , ro-ex:GuidanceRequirement ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Ontology_Specifics_Not_Exploited ;
      ro-ex:justifies ro-ex:RestrictiveToolUsableSchema ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:ControlOrBehaviourConfigurability
      rdf:type ro:FunctionalRequirement , ro-ex:CustomisationRequirement ;
      rdfs:comment "Interaction configurability"^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Interaction ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:CustomisationRequirement
      rdf:type owl:Class ;
      rdfs:subClassOf ro-ex:SemVisRequirement .

ro-ex:DataFiltering
      rdf:type ro:FunctionalRequirement , ro-ex:OptionalFeature ;
      ro-ex:justifiedBy ro-ex:Benefits_of_interaction_for_information_visualisation_not_exploited ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:DataLanguageRequirement
      rdf:type owl:Class ;
      rdfs:subClassOf ro-ex:RVLRequirement .

ro-ex:DataRequirement
      rdf:type owl:Class ;
      rdfs:subClassOf ro-ex:SemVisRequirement .

ro-ex:Declarative
      rdf:type ro-ex:GeneralLanguageRequirement ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:ExplicitMappingDefinition , ro-ex:PlatformVariability ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:DomainAgnostic
      rdf:type ro:FunctionalRequirement , ro-ex:DataRequirement ;
      dcterms:description "Data from every domain can be visualised. There is at least some support for visualisation that is independent from the domain."^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:printOrder "4"^^xsd:float .

ro-ex:DomainExpertsCanUseTheSystem
      rdf:type ro:FunctionalRequirement , ro-ex:OptionalFeature ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies , ro-ex:semVisApproach ;
      ro-ex:printOrder "2"^^xsd:float .

ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:comment "We cannot assume that domain experts are also visualisation experts."^^xsd:string ;
      dcterms:description "Domain Experts cannot easily create visualisations of their domain ontologies."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:DomainAgnostic , ro-ex:DomainExpertsCanUseTheSystem , ro-ex:GUIforVisualEncodingConfiguration , ro-ex:Guidance4DataSelection , ro-ex:Guidance4ViewTransformations , ro-ex:Guidance4VisualMapping , ro-ex:Guidance4VisualMappingWithAGUI ;
      ro-ex:printOrder "9"^^xsd:float ;
      ro-ex:solvedBy ro-ex:SemVisApproach , ro-ex:VISO .

ro-ex:Exchange_of_Visualisation_Knowledge
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:comment "Visualisation Knowledge is hard-coded to the system and sometimes implicitly contained in system defaults. Since it is not made explicit, it cannot easily be exchanged when new empirical results are at hand. Also, it cannot be shared easily with others."^^xsd:string ;
      dcterms:description "Visualisation knowledge is not exchangeable and shareable."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:printOrder "7"^^xsd:float ;
      ro-ex:solvedBy ro-ex:RVL , ro-ex:VISO .

ro-ex:Experimenting_with_visualisation_settings_is_tedious_in_standard_pipeline_approaches
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:comment "Experimenting with new visualisation settings (filtering data, changing the visual mapping) is often tedious, since the results of configuring the visualisation are not visible immediatledy but only after running through all step s of the visualisation pipeline. Changes done to other parts (e.g. view transformations) are often lost."^^xsd:string ;
      dcterms:description "No incremental, iterative, immediate application of configuration changes. Experimenting is tedious."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:printOrder "8"^^xsd:float .

ro-ex:ExplicitMappingDefinition
      rdf:type ro:FunctionalRequirement , ro-ex:VariabilityReuseAndComposabilityRequirement ;
      rdfs:label "Explicit mapping definition"^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:RVL , ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:No_Sharing_and_Reuse_of_Visual_Encodings ;
      ro-ex:justifies ro-ex:Declarative ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:ExtensibilityOfMappings
      rdf:type ro-ex:VariabilityShareabilityAndComposabilityLanguageRequirement ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:ReusabilityOfMappings ;
      ro-ex:printOrder "5"^^xsd:float .

ro-ex:FamiliarToUsers
      rdf:type ro-ex:GeneralLanguageRequirement ;
      ro-ex:printOrder "3"^^xsd:float ;
      skos:editorialNote "Löschen? Evtl. nur diskutieren, ob Requirement?"^^xsd:string .

ro-ex:GUIforVisualEncodingConfiguration
      rdf:type ro:FunctionalRequirement , ro-ex:CustomisationRequirement ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:justifies ro-ex:RestrictiveToolUsableSchema ;
      ro-ex:printOrder "2"^^xsd:float .

ro-ex:GeneralLanguageRequirement
      rdf:type owl:Class ;
      rdfs:subClassOf ro-ex:RVLRequirement .

ro-ex:Guidance4DataSelection
      rdf:type ro-ex:OptionalFeature ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:printOrder "4"^^xsd:float .

ro-ex:Guidance4ViewTransformations
      rdf:type ro-ex:OptionalFeature ;
      ro-ex:justifiedBy ro-ex:Benefits_of_interaction_for_information_visualisation_not_exploited , ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:printOrder "5"^^xsd:float .

ro-ex:Guidance4VisualMapping
      rdf:type ro:FunctionalRequirement , ro-ex:GuidanceRequirement ;
      ro:refinesTo ro-ex:Guidance4VisualMappingWithAGUI ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:justifies ro-ex:IntrospectCreatedVisualisation , ro-ex:RestrictiveToolUsableSchema ;
      ro-ex:printOrder "2"^^xsd:float .

ro-ex:Guidance4VisualMappingWithAGUI
      rdf:type ro:FunctionalRequirement , ro-ex:GuidanceRequirement ;
      ro:isRefinementOf ro-ex:Guidance4VisualMapping ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Domain_experts_can_not_easily_visualise_domain_ontologies ;
      ro-ex:justifies ro-ex:RestrictiveToolUsableSchema ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:GuidanceRequirement
      rdf:type owl:Class ;
      rdfs:subClassOf ro-ex:SemVisRequirement .

ro-ex:How_can_we_guide_the_visual_mapping_of_ontological_data
      rdf:type ro-ex:ResearchQuestion ;
      rdfs:label "How can we guide the visual mapping of ontological data"^^xsd:string ;
      ro:isRefinementOf ro-ex:How_can_we_support_the_creation_of_tailor-made_efficient_and_reusable_visualisations_for_ontological_data ;
      ro-ex:justifiedBy ro-ex:Exchange_of_Visualisation_Knowledge , ro-ex:Ontology_Specifics_Not_Exploited ;
      ro-ex:printOrder "6"^^xsd:float .

ro-ex:How_can_we_support_the_creation_of_tailor-made_efficient_and_reusable_visualisations_for_ontological_data
      rdf:type ro-ex:ResearchQuestion ;
      rdfs:label "How can we support the creation of tailor-made  efficient and reusable visualisations for ontological data?"^^xsd:string ;
      ro-ex:printOrder "0"^^xsd:float .

ro-ex:How_to_define_composable_and_shareable_mappings_from_ontological_data_to_visual_means
      rdf:type ro-ex:ResearchQuestion ;
      rdfs:label "How to define composable and shareable mappings from ontological data to visual means"^^xsd:string ;
      ro:isRefinementOf ro-ex:How_can_we_support_the_creation_of_tailor-made_efficient_and_reusable_visualisations_for_ontological_data ;
      ro-ex:justifiedBy ro-ex:Composition_of_Mappings_insufficient , ro-ex:No_Sharing_and_Reuse_of_Visual_Encodings , ro-ex:No_Standard_for_Visualisation_Information , ro-ex:No_Visualisation_Language_for_Ontologies , ro-ex:Single_Visual_Paradigm ;
      ro-ex:printOrder "5"^^xsd:float .

ro-ex:How_to_formalise_graphic_vocabulary_and_knowledge
      rdf:type ro-ex:ResearchQuestion ;
      rdfs:label "How to formalise graphic vocabulary and knowledge"^^xsd:string ;
      ro:isRefinementOf ro-ex:How_can_we_support_the_creation_of_tailor-made_efficient_and_reusable_visualisations_for_ontological_data ;
      ro-ex:justifiedBy ro-ex:Exchange_of_Visualisation_Knowledge ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:InSomeFieldsMidLevelOntologiesAreUsedAndSemanticsEmergeFromReusingThemInManyDomainOntologies
      rdf:type ro-ex:CaseStudyOntologyAnalysisObservation ;
      rdfs:label "In some fields mid level ontologies are used and semantics emerge from reusing them in many domain ontologies"^^xsd:string .

ro-ex:Interaction
      rdf:type ro:FunctionalRequirement , ro-ex:VisualEncodingAndInteractivityRequirement ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:Benefits_of_interaction_for_information_visualisation_not_exploited ;
      ro-ex:justifies ro-ex:ConfigurationResultsInstantlyShown , ro-ex:ControlOrBehaviourConfigurability , ro-ex:InteractionDescribeable ;
      ro-ex:printOrder "2"^^xsd:float .

ro-ex:InteractionDescribeable
      rdf:type ro-ex:VisualEncodingAndInteractivityLanguageRequirement ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:Interaction ;
      ro-ex:printOrder "0"^^xsd:float .

ro-ex:IntrospectCreatedVisualisation
      rdf:type ro-ex:GuidanceRequirement ;
      rdfs:label "Introspect created visualisation"^^xsd:string ;
      dcterms:description "Introspecting the already performed (composed visualisation) is required in order to allow making statements on further (appropriate or even possible) compositions."^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:AVM ;
      ro-ex:justifiedBy ro-ex:ComposabilityOfTheDefinedMapping , ro-ex:Guidance4VisualMapping ;
      ro-ex:printOrder "6"^^xsd:float .

ro-ex:It_is_so_much_work_to_create_a_good_visualisation_that_it_would_be_nice_to_reuse_the_settings_in_multiple_settings_eg_on_other_platforms
      rdf:type ro-ex:MockupObservation ;
      rdfs:label " on other platforms"^^xsd:string .

ro-ex:It_would_be_nice_to_reuse_already_created_parts_of_visualisations_in_other_contexts_to_save_time_and_have_a_consistent_appearance
      rdf:type ro-ex:MockupObservation ;
      rdfs:label "It would be nice to reuse already created parts of visualisations in other contexts to save time and have a consistent appearance"^^xsd:string .

ro-ex:It_would_save_time_to_employ_ready_to_use_visualisation_and_styling_settings_for_standard_relations_classes_and_instances
      rdf:type ro-ex:MockupObservation ;
      rdfs:label "It would save time to employ ready to use visualisation and styling settings for standard relations classes and instances"^^xsd:string .

ro-ex:MockupObservation
      rdf:type owl:Class ;
      rdfs:label "Mockup observation"^^xsd:string ;
      rdfs:subClassOf ro-ex:CaseStudyObservation .

ro-ex:MultipleVS
      rdf:type ro-ex:VariabilityShareabilityAndComposabilityLanguageRequirement ;
      ro:refinesTo ro-ex:VisualStructureIndependence ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:VisualStructureVariability ;
      ro-ex:printOrder "3"^^xsd:float .

ro-ex:NeedForDirectlyReferencingOntologyTerms
      rdf:type ro-ex:CaseStudyOntologyAnalysisObservation ;
      rdfs:label "Need for directly referencing ontology terms"^^xsd:string .

ro-ex:NeedForVisualingRelationsBetweenComplexObjectsRatherThanAttributeValuesAlone
      rdf:type ro-ex:CaseStudyOntologyAnalysisObservation ;
      rdfs:label "Need for visualing relations between complex objects rather than attribute values alone"^^xsd:string .

ro-ex:NeedForVisualisingManyDiffererntRelationshipsWithLittleOccurencesHeterogeneousData
      rdf:type ro-ex:CaseStudyOntologyAnalysisObservation ;
      rdfs:label "Need for visualising many differernt relationships with little occurences (heterogeneous data)"^^xsd:string .

ro-ex:No_Real_Visual_Encoding
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:comment "Setting concrete visual values for all data values is time-consuming and unflexible in case of changes. A reasonable selection of defaults, following the convention over configuration principle,  is often not possible."^^xsd:string ;
      dcterms:description "Often no real visual encoding but transformation of data to document shape (for ontological data)."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:RealVisualEncoding , ro-ex:VisualEncodingConfigurability ;
      ro-ex:printOrder "1"^^xsd:float ;
      ro-ex:solvedBy ro-ex:RVL , ro-ex:SemVisApproach .

ro-ex:No_Sharing_and_Reuse_of_Visual_Encodings
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      rdfs:comment "Even if there are explicit visual mapping definitions, it may still be the case that these cannot easily be shared together with the data, because they are stored in different formats."^^xsd:string , "Ontology visualisations are created for specific platforms and separatly for each visual paradigm. They cannot be shared with others and reused on other visualisation platforms or in combination with different visual paradigms."^^xsd:string ;
      dcterms:description "Visualisation authors cannot reuse mappings in other tools and share them with other people."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:ExplicitMappingDefinition , ro-ex:PlatformVariability , ro-ex:ReusabilityOfMappings , ro-ex:ShareabilityOfMappings , ro-ex:VisualStructureVariability ;
      ro-ex:printOrder "3"^^xsd:float ;
      ro-ex:solvedBy ro-ex:AVM , ro-ex:RVL , ro-ex:VISO .

ro-ex:No_Standard_for_Visualisation_Information
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      dcterms:description "Domain data authors do not have a standard format to ship also visualisation information along with their data."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:justifies ro-ex:OntologyAware , ro-ex:RDFSupportedByApproach ;
      ro-ex:printOrder "5"^^xsd:float ;
      ro-ex:solvedBy ro-ex:RVL .

ro-ex:No_Visualisation_Language_for_Ontologies
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
      dcterms:description "No visualisation language for ontological data exists."^^xsd:string ;
      ro-ex:justifiedBy ro-ex:someRechercheResult ;
      ro-ex:printOrder "6"^^xsd:float ;
      ro-ex:solvedBy ro-ex:RVL , ro-ex:VISO ;
      skos:editorialNote "TODO: Is this a language problem?"^^xsd:string .

ro-ex:Often_we_want_to_map_whole_properties_to_visual_means
      rdf:type ro-ex:MockupObservation ;
      rdfs:label "Often we want to map whole properties to visual means"^^xsd:string ;
      dcterms:description "Often we do ot want to map each domain data value to a visual value, but do this implicitly by mapping on the property level. This applies to visual attributes but other visual relations as well."^^xsd:string .

ro-ex:OntologyAware
      rdf:type ro:FunctionalRequirement , ro-ex:DataRequirement ;
      dcterms:description "Ontology terms can be referenced within display definitions. The visualisation is specific to ontologies not graphs, i.e. representing RDF on a graph level is not sufficient."^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:SemVisApproach ;
      ro-ex:justifiedBy ro-ex:No_Standard_for_Visualisation_Information , ro-ex:Ontology_Specifics_Not_Exploited ;
      ro-ex:justifies ro-ex:OntologyConstructsReferenceable ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:OntologyConstructsReferenceable
      rdf:type ro-ex:DataLanguageRequirement ;
      dcterms:description "The language can reference ontology terms. Switching on/off inference is optional."^^xsd:string ;
      ro-ex:fulfilledBy ro-ex:RVL ;
      ro-ex:justifiedBy ro-ex:OntologyAware ;
      ro-ex:printOrder "1"^^xsd:float .

ro-ex:Ontology_Specifics_Not_Exploited
      rdf:type ro:Challenge , ro-ex:RechercheResult ;
