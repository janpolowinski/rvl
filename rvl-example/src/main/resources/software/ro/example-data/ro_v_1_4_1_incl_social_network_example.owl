        <owl:disjointWith rdf:resource="#UEfficiency"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
        <owl:disjointWith rdf:resource="#UserSatisfaction"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#UEfficiency -->

    <owl:Class rdf:about="#UEfficiency">
        <rdfs:subClassOf rdf:resource="#Usability"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
