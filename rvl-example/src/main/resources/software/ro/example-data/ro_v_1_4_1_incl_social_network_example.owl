        <owl:disjointWith rdf:resource="#PeripheralDevice"/>
        <owl:disjointWith rdf:resource="#SystemSoftware"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#HardwareInfrastructure -->

    <owl:Class rdf:about="#HardwareInfrastructure">
