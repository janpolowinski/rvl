    <!-- http://purl.org/ro/ont#UseCase -->

    <owl:Class rdf:about="#UseCase">
        <rdfs:subClassOf rdf:resource="#Story"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasScenario"/>
                <owl:someValuesFrom rdf:resource="#Scenario"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#describesRequirement"/>
                <owl:someValuesFrom rdf:resource="#Requirement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
