    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Risk -->

    <owl:Class rdf:about="#Risk">
        <rdfs:subClassOf rdf:resource="#Problem"/>
        <rdfs:comment
            >The quantifiable likelihood of loss or less-than-expected returns.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Risky -->

    <owl:Class rdf:about="#Risky">
        <rdfs:subClassOf rdf:resource="#State"/>
        <rdfs:comment
            >All requirements that have a risk automatically become &#39;Risky&#39;. This cannot be set by the user.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Scenario -->

    <owl:Class rdf:about="#Scenario">
        <rdfs:subClassOf rdf:resource="#Story"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongsToUseCase"/>
                <owl:someValuesFrom rdf:resource="#UseCase"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A textual description of a small part of a Use-Case. A scenario describes a sequence of user actions in general terms.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Security -->

    <owl:Class rdf:about="#Security">
        <rdfs:subClassOf rdf:resource="#Functionality"/>
