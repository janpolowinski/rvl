        <owl:disjointWith rdf:resource="#Operability"/>
        <owl:disjointWith rdf:resource="#Suitability"/>
        <owl:disjointWith rdf:resource="#UCompliance"/>
        <owl:disjointWith rdf:resource="#UEfficiency"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
        <owl:disjointWith rdf:resource="#UserSatisfaction"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#LowCost -->

    <owl:Class rdf:about="#LowCost">
        <rdfs:subClassOf rdf:resource="#Cost"/>
