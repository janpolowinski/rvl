        <owl:disjointWith rdf:resource="#Security"/>
        <owl:disjointWith rdf:resource="#Suitability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Invalid -->

    <owl:Class rdf:about="#Invalid">
        <rdfs:subClassOf rdf:resource="#State"/>
        <owl:disjointWith rdf:resource="#Valid"/>
