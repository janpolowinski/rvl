        <owl:disjointWith rdf:resource="#LowPriority"/>
        <owl:disjointWith rdf:resource="#MediumPriority"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Host -->

    <owl:Class rdf:about="#Host">
        <rdfs:subClassOf rdf:resource="#PlatformRequirement"/>
