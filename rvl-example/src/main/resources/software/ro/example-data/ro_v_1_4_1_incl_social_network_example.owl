        <rdfs:comment
            >Requirements regarding to the Platform of the software to be developed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Portability -->

    <owl:Class rdf:about="#Portability">
        <rdfs:subClassOf rdf:resource="#NonFunctionalRequirement"/>
        <owl:disjointWith rdf:resource="#Reliability"/>
        <owl:disjointWith rdf:resource="#Usability"/>
        <rdfs:comment rdf:datatype="&xsd;string"
