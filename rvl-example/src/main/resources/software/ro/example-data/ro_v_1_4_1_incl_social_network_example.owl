    <!-- http://purl.org/ro/ont#TestCase -->

    <owl:Class rdf:about="#TestCase">
        <rdfs:subClassOf rdf:resource="#Artifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#testsRequirement"/>
                <owl:someValuesFrom rdf:resource="#Requirement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A Test-Case defines how to test a requirement after implementation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Testability -->

    <owl:Class rdf:about="#Testability">
        <rdfs:subClassOf rdf:resource="#Maintainability"/>
