<?xml version="1.0"?>

<!-- 
 ##################### ##################### ##################### ##################### ##################### #####################
 This is a copy of the ontology that we store here in a specific version for (offline) testing purposes.
 Besides the requirements ontology itself, this file also contains exemplary instance data (requirments for a social network application)
 All rights remain at the authors. 
 Please refer to http://purl.org/ro/ont# or cf. Siegemund, Katja, et al. "Towards ontology-driven requirements engineering." 
 Workshop Semantic Web Enabled Software Engineering at 10th International Semantic Web Conference (ISWC), Bonn. 2011
 for further information.
 
 The following changes have been done to instance data in order to be able to demonstrate all use cases:
 - added some values for responseTimeInMs as literals typed xsd:integer. 
 
 ##################### ##################### ##################### ##################### ##################### #####################
 -->

<!DOCTYPE rdf:RDF [
    <!ENTITY ont "http://purl.org/ro/ont#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://purl.org/ro/ont#"
     xml:base="http://purl.org/ro/ont"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:ont="http://purl.org/ro/ont#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ro/ont#belongsToUseCase -->

    <owl:ObjectProperty rdf:about="#belongsToUseCase">
        <rdfs:domain rdf:resource="#Scenario"/>
        <rdfs:range rdf:resource="#UseCase"/>
        <owl:inverseOf rdf:resource="#hasScenario"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#describesRequirement -->

    <owl:ObjectProperty rdf:about="#describesRequirement">
        <rdfs:range rdf:resource="#Requirement"/>
        <rdfs:domain rdf:resource="#UseCase"/>
        <owl:inverseOf rdf:resource="#isDescribedByUseCase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasAbstractRequirement -->

    <owl:ObjectProperty rdf:about="#hasAbstractRequirement">
        <rdfs:range rdf:resource="#AbstractFunctionalRequirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasCost -->

    <owl:ObjectProperty rdf:about="#hasCost">
        <rdf:type rdf:resource="&owl;Class"/>
        <owl:objectPropertyRange rdf:resource="#Cost"/>
        <owl:objectPropertyDomain rdf:resource="#hasCost"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasFailurePostcondition -->

    <owl:ObjectProperty rdf:about="#hasFailurePostcondition">
        <rdfs:range rdf:resource="#FailurePostcondition"/>
        <rdfs:domain rdf:resource="#Requirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasGoal -->

    <owl:ObjectProperty rdf:about="#hasGoal">
        <rdfs:range rdf:resource="#Goal"/>
        <rdfs:domain rdf:resource="#Requirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasObstacle -->

    <owl:ObjectProperty rdf:about="#hasObstacle">
        <rdfs:range rdf:resource="#Obstacle"/>
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasRefinementSource -->

    <owl:ObjectProperty rdf:about="#hasRefinementSource">
        <rdfs:domain rdf:resource="#Refinement"/>
        <rdfs:range rdf:resource="#Requirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasRefinementTarget -->

    <owl:ObjectProperty rdf:about="#hasRefinementTarget">
        <rdfs:domain rdf:resource="#Refinement"/>
        <rdfs:range rdf:resource="#Requirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasRelationship -->

    <owl:ObjectProperty rdf:about="#hasRelationship">
        <rdfs:comment
            >Describes the kind of relationship between Requirement Artifacts, usually between requirements.</rdfs:comment>
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="#Requirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasRisk -->

    <owl:ObjectProperty rdf:about="#hasRisk">
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
        <rdfs:range rdf:resource="#Risk"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasScenario -->

    <owl:ObjectProperty rdf:about="#hasScenario">
        <rdfs:range rdf:resource="#Scenario"/>
        <rdfs:domain rdf:resource="#UseCase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasSoftMetric -->

    <owl:ObjectProperty rdf:about="#hasSoftMetric">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="#SoftMetric"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasSource -->

    <owl:ObjectProperty rdf:about="#hasSource">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Source"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasTestCase -->

    <owl:ObjectProperty rdf:about="#hasTestCase">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="#TestCase"/>
        <owl:inverseOf rdf:resource="#testsRequirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasTrigger -->

    <owl:ObjectProperty rdf:about="#hasTrigger">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="#Trigger"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#hasVerificationMethod -->

    <owl:ObjectProperty rdf:about="#hasVerificationMethod"/>
    


    <!-- http://purl.org/ro/ont#isAlternativeTo -->

    <owl:ObjectProperty rdf:about="#isAlternativeTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:range rdf:resource="#Requirement"/>
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isCoexistentWith"/>
        <owl:propertyDisjointWith rdf:resource="#isGeneralizationOf"/>
        <owl:propertyDisjointWith rdf:resource="#isInConflictWith"/>
        <owl:propertyDisjointWith rdf:resource="#isRefinementOf"/>
        <owl:propertyDisjointWith rdf:resource="#isRevisionOf"/>
        <owl:propertyDisjointWith rdf:resource="#isSpecializationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isAuthoredBy -->

    <owl:ObjectProperty rdf:about="#isAuthoredBy">
        <rdfs:domain rdf:resource="#Artifact"/>
        <rdfs:range rdf:resource="#Stakeholder"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isCoexistentWith -->

    <owl:ObjectProperty rdf:about="#isCoexistentWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isExclusionOf"/>
        <owl:propertyDisjointWith rdf:resource="#isInConflictWith"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isConnectedToUseCase -->

    <owl:ObjectProperty rdf:about="#isConnectedToUseCase">
        <rdfs:domain rdf:resource="#Goal"/>
        <rdfs:range rdf:resource="#UseCase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isDescribedByUseCase -->

    <owl:ObjectProperty rdf:about="#isDescribedByUseCase">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="#UseCase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isExclusionOf -->

    <owl:ObjectProperty rdf:about="#isExclusionOf">
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isGeneralizationOf"/>
        <owl:propertyDisjointWith rdf:resource="#isRefinementOf"/>
        <owl:propertyDisjointWith rdf:resource="#isRevisionOf"/>
        <owl:propertyDisjointWith rdf:resource="#isSpecializationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isGeneralizationOf -->

    <owl:ObjectProperty rdf:about="#isGeneralizationOf">
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isInConflictWith"/>
        <owl:propertyDisjointWith rdf:resource="#isRefinementOf"/>
        <owl:propertyDisjointWith rdf:resource="#isSpecializationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isInConflictWith -->

    <owl:ObjectProperty rdf:about="#isInConflictWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isRefinementOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isNegativeContributionTo -->

    <owl:ObjectProperty rdf:about="#isNegativeContributionTo">
        <rdfs:range rdf:resource="#RequirementArtifact"/>
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
        <owl:propertyDisjointWith rdf:resource="#isPositiveContributionTo"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isPositiveContributionTo -->

    <owl:ObjectProperty rdf:about="#isPositiveContributionTo">
        <rdfs:range rdf:resource="#RequirementArtifact"/>
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isRefinementOf -->

    <owl:ObjectProperty rdf:about="#isRefinementOf">
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
        <owl:propertyDisjointWith rdf:resource="#isRevisionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isRevisionOf -->

    <owl:ObjectProperty rdf:about="#isRevisionOf">
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#isSpecializationOf -->

    <owl:ObjectProperty rdf:about="#isSpecializationOf">
        <rdfs:subPropertyOf rdf:resource="#hasRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#refinesTo -->

    <owl:ObjectProperty rdf:about="#refinesTo">
        <rdfs:range rdf:resource="#RequirementArtifact"/>
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#testsRequirement -->

    <owl:ObjectProperty rdf:about="#testsRequirement">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:range rdf:resource="#Requirement"/>
        <rdfs:domain rdf:resource="#TestCase"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ro/ont#throwsException -->

    <owl:ObjectProperty rdf:about="#throwsException">
        <rdfs:domain rdf:resource="#Exception"/>
        <rdfs:range rdf:resource="#Exception"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ro/ont#hasCost -->

    <owl:DatatypeProperty rdf:about="#hasCost">
        <owl:dataPropertyRange rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasID -->

    <owl:DatatypeProperty rdf:about="#hasID">
        <rdfs:comment
            >The Identifier for a Requirement Artifact, e.g.

FR1 or Functional Requirement
G1 for Goal
NFR2.2 for a refined Non-Functional Requirement
R7 for a Risk, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="#RequirementArtifact"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasRefinementReason -->

    <owl:DatatypeProperty rdf:about="#hasRefinementReason">
        <rdfs:domain rdf:resource="#Refinement"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasResponseTime -->

    <owl:DatatypeProperty rdf:about="#hasResponseTime">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:subPropertyOf rdf:resource="#hasTimingMetric"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasThroughput -->

    <owl:DatatypeProperty rdf:about="#hasThroughput">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:subPropertyOf rdf:resource="#hasTimingMetric"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasTimingMetric -->

    <owl:DatatypeProperty rdf:about="#hasTimingMetric">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ro/ont#hasWaitingTime -->

    <owl:DatatypeProperty rdf:about="#hasWaitingTime">
        <rdfs:domain rdf:resource="#Requirement"/>
        <rdfs:subPropertyOf rdf:resource="#hasTimingMetric"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ro/ont#AbstractFunctionalRequirement -->

    <owl:Class rdf:about="#AbstractFunctionalRequirement">
        <rdfs:subClassOf rdf:resource="#FunctionalRequirement"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Accuracy -->

    <owl:Class rdf:about="#Accuracy">
        <rdfs:subClassOf rdf:resource="#Functionality"/>
        <owl:disjointWith rdf:resource="#FCompliance"/>
        <owl:disjointWith rdf:resource="#Interoperability"/>
        <owl:disjointWith rdf:resource="#Security"/>
        <owl:disjointWith rdf:resource="#Suitability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Adaptability -->

    <owl:Class rdf:about="#Adaptability">
        <rdfs:subClassOf rdf:resource="#Portability"/>
        <owl:disjointWith rdf:resource="#Coexistence"/>
        <owl:disjointWith rdf:resource="#Conformance"/>
        <owl:disjointWith rdf:resource="#Installability"/>
        <owl:disjointWith rdf:resource="#Interoperability"/>
        <owl:disjointWith rdf:resource="#PCompliance"/>
        <owl:disjointWith rdf:resource="#Replaceability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Analysis -->

    <owl:Class rdf:about="#Analysis">
        <rdfs:subClassOf rdf:resource="#VerificationMethod"/>
        <rdfs:comment
            >The requirements functionality will be verified by analysis.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Analyzability -->

    <owl:Class rdf:about="#Analyzability">
        <rdfs:subClassOf rdf:resource="#Maintainability"/>
        <owl:disjointWith rdf:resource="#Changeability"/>
        <owl:disjointWith rdf:resource="#FCompliance"/>
        <owl:disjointWith rdf:resource="#MCompliance"/>
        <owl:disjointWith rdf:resource="#Stability"/>
        <owl:disjointWith rdf:resource="#Testability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Artifact -->

    <owl:Class rdf:about="#Artifact">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAuthoredBy"/>
                <owl:someValuesFrom rdf:resource="#Stakeholder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >Requirement Assets that appear during Requirements Engineering tasks.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#AtomicFunctionalRequirement -->

    <owl:Class rdf:about="#AtomicFunctionalRequirement">
        <rdfs:subClassOf rdf:resource="#FunctionalRequirement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAbstractRequirement"/>
                <owl:someValuesFrom rdf:resource="#AbstractFunctionalRequirement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Attractiveness -->

    <owl:Class rdf:about="#Attractiveness">
        <rdfs:subClassOf rdf:resource="#Usability"/>
        <owl:disjointWith rdf:resource="#Effectiveness"/>
        <owl:disjointWith rdf:resource="#Learnability"/>
        <owl:disjointWith rdf:resource="#Operability"/>
        <owl:disjointWith rdf:resource="#UCompliance"/>
        <owl:disjointWith rdf:resource="#UEfficiency"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
        <owl:disjointWith rdf:resource="#UserSatisfaction"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Attribute -->

    <owl:Class rdf:about="#Attribute">
        <rdfs:comment
            >Additional data for Requirement Artifacts.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Challenge -->

    <owl:Class rdf:about="#Challenge">
        <rdfs:subClassOf rdf:resource="#RequirementArtifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasObstacle"/>
                <owl:someValuesFrom rdf:resource="#Obstacle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAuthoredBy"/>
                <owl:someValuesFrom rdf:resource="#Stakeholder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#refinesTo"/>
                <owl:someValuesFrom rdf:resource="#RequirementArtifact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRisk"/>
                <owl:someValuesFrom rdf:resource="#Risk"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Goal"/>
        <owl:disjointWith rdf:resource="#Requirement"/>
        <owl:disjointWith rdf:resource="#Story"/>
        <rdfs:comment
            >An act or statement of defiance regarding the project objectives.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Changeability -->

    <owl:Class rdf:about="#Changeability">
        <rdfs:subClassOf rdf:resource="#Maintainability"/>
        <owl:disjointWith rdf:resource="#FCompliance"/>
        <owl:disjointWith rdf:resource="#MCompliance"/>
        <owl:disjointWith rdf:resource="#Stability"/>
        <owl:disjointWith rdf:resource="#Testability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Checked -->

    <owl:Class rdf:about="#Checked">
        <rdfs:subClassOf rdf:resource="#State"/>
        <owl:disjointWith rdf:resource="#Unchecked"/>
        <rdfs:comment
            >The status of a requirement is automatically set to &#39;checked&#39; if it has been checked.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Coexistence -->

    <owl:Class rdf:about="#Coexistence">
        <rdfs:subClassOf rdf:resource="#Portability"/>
        <owl:disjointWith rdf:resource="#Conformance"/>
        <owl:disjointWith rdf:resource="#Installability"/>
        <owl:disjointWith rdf:resource="#PCompliance"/>
        <owl:disjointWith rdf:resource="#Replaceability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Complete -->

    <owl:Class rdf:about="#Complete">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Checked"/>
                    <rdf:Description rdf:about="#Implemented"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#Incomplete"/>
        <rdfs:comment
            >The status of the requirement automatically becomes &#39;complete&#39; if it was checked and has been implemented.  This cannot be set by the user.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Condition -->

    <owl:Class rdf:about="#Condition">
        <rdfs:subClassOf rdf:resource="#Attribute"/>
        <rdfs:comment></rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Conformance -->

    <owl:Class rdf:about="#Conformance">
        <rdfs:subClassOf rdf:resource="#Portability"/>
        <owl:disjointWith rdf:resource="#Installability"/>
        <owl:disjointWith rdf:resource="#Interoperability"/>
        <owl:disjointWith rdf:resource="#PCompliance"/>
        <owl:disjointWith rdf:resource="#Replaceability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Cost -->

    <owl:Class rdf:about="#Cost">
        <rdfs:subClassOf rdf:resource="#Attribute"/>
        <rdfs:comment
            >Financial cost for a Requirement Artifact, usually for a requirement.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Demonstration -->

    <owl:Class rdf:about="#Demonstration">
        <rdfs:subClassOf rdf:resource="#VerificationMethod"/>
        <rdfs:comment
            >The requirements functionality will be verified by demonstration.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#ECompliance -->

    <owl:Class rdf:about="#ECompliance">
        <rdfs:subClassOf rdf:resource="#Efficiency"/>
        <owl:disjointWith rdf:resource="#ResourceBehaviour"/>
        <owl:disjointWith rdf:resource="#TimeBehaviour"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Effectiveness -->

    <owl:Class rdf:about="#Effectiveness">
        <rdfs:subClassOf rdf:resource="#Usability"/>
        <owl:disjointWith rdf:resource="#Learnability"/>
        <owl:disjointWith rdf:resource="#Operability"/>
        <owl:disjointWith rdf:resource="#UCompliance"/>
        <owl:disjointWith rdf:resource="#UEfficiency"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
        <owl:disjointWith rdf:resource="#UserSatisfaction"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Efficiency -->

    <owl:Class rdf:about="#Efficiency">
        <rdfs:subClassOf rdf:resource="#NonFunctionalRequirement"/>
        <owl:disjointWith rdf:resource="#Functionality"/>
        <owl:disjointWith rdf:resource="#Maintainability"/>
        <owl:disjointWith rdf:resource="#Portability"/>
        <owl:disjointWith rdf:resource="#Reliability"/>
        <owl:disjointWith rdf:resource="#Usability"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A set of attributes that bear on the relationship between the level of performance of the software and the amount of resources used, under stated conditions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Exception -->

    <owl:Class rdf:about="#Exception">
        <rdfs:comment
            >Exceptions occur as a result of any Completeness, Consistency or Quality check. Exceptions will be automatically stored in this class as Individuals. This class shall not be visible to the user, except when querying for Exceptions. Exception cannot be added manually by the user.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#FCompliance -->

    <owl:Class rdf:about="#FCompliance">
        <rdfs:subClassOf rdf:resource="#Functionality"/>
        <owl:disjointWith rdf:resource="#Interoperability"/>
        <owl:disjointWith rdf:resource="#Security"/>
        <owl:disjointWith rdf:resource="#Stability"/>
        <owl:disjointWith rdf:resource="#Suitability"/>
        <owl:disjointWith rdf:resource="#Testability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#FailurePostcondition -->

    <owl:Class rdf:about="#FailurePostcondition">
        <rdfs:subClassOf rdf:resource="#Condition"/>
        <rdfs:comment
            >A condition that is caused by faulty behaviour.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#FaultTolerance -->

    <owl:Class rdf:about="#FaultTolerance">
        <rdfs:subClassOf rdf:resource="#Reliability"/>
        <owl:disjointWith rdf:resource="#Maturity"/>
        <owl:disjointWith rdf:resource="#RCompliance"/>
        <owl:disjointWith rdf:resource="#Recoverability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#FunctionalRequirement -->

    <owl:Class rdf:about="#FunctionalRequirement">
        <rdfs:subClassOf rdf:resource="#Requirement"/>
        <owl:disjointWith rdf:resource="#NonFunctionalRequirement"/>
        <owl:disjointWith rdf:resource="#PlatformRequirement"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Functional Requirement defines a function of a software system or its component. It can be classified into goal, feature and atomic.</rdfs:comment>
