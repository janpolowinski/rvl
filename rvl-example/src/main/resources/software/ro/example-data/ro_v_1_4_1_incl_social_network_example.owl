    </owl:Class>
    


    <!-- http://purl.org/ro/ont#RCompliance -->

    <owl:Class rdf:about="#RCompliance">
        <rdfs:subClassOf rdf:resource="#Reliability"/>
        <owl:disjointWith rdf:resource="#Recoverability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Recoverability -->

    <owl:Class rdf:about="#Recoverability">
        <rdfs:subClassOf rdf:resource="#Reliability"/>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Refinement -->

    <owl:Class rdf:about="#Refinement">
