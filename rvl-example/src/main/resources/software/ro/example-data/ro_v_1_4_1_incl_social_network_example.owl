    </owl:Class>
    


    <!-- http://purl.org/ro/ont#State -->

    <owl:Class rdf:about="#State">
        <rdfs:subClassOf rdf:resource="#Attribute"/>
        <rdfs:comment
            >The status of a Requirement Artifact.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Story -->

    <owl:Class rdf:about="#Story">
        <rdfs:subClassOf rdf:resource="#RequirementArtifact"/>
        <rdfs:comment
            >Use-Cases and Scenarios.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ro/ont#Suitability -->

    <owl:Class rdf:about="#Suitability">
        <rdfs:subClassOf rdf:resource="#Functionality"/>
        <owl:disjointWith rdf:resource="#Understandability"/>
