:targetValue
      rdfs:range 
      	viso-graphic:Visual_Attribute , rdfs:Literal;
      rdfs:domain
      	:ValueMapping
#	  , :PropertyToGraphicAttributeMapping;
      . 
      
:sourceValue
      rdfs:domain 
      	:ValueMapping 
